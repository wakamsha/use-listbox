{
  "name": "@wakamsha/use-listbox",
  "version": "1.0.1",
  "description": "A simple React Hook that Provides the behavior and accessibility implementation for a ListBox component.",
  "keywords": [
    "react",
    "react hooks",
    "accessible",
    "accessibility",
    "a11y",
    "wai-aria",
    "dropdown",
    "menu",
    "contextmenu"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:wakamsha/use-listbox.git",
  "author": "wakamsha <wakamsha@gmail.com> (https://twitter.com/wakamsha)",
  "license": "MIT",
  "engines": {
    "node": "16.18.0"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "  prettier -c    src/**/*.{ts,tsx}; eslint -c .eslintrc.js       src/**/*.{ts,tsx}",
    "format": "prettier -c -w src/**/*.{ts,tsx}; eslint -c .eslintrc.js --fix src/**/*.{ts,tsx}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "tsc --project tsconfig.build.json --declaration",
    "prepublishOnly": "npm run build",
    "predeploy": "build-storybook",
    "deploy": "gh-pages -d storybook-static"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@emotion/css": "11.10.5",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/react": "6.5.12",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@types/react": "17.0.50",
    "@types/react-dom": "17.0.17",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "babel-loader": "8.2.5",
    "cspell": "6.12.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.6",
    "gh-pages": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "postcss-syntax": "0.36.2",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "stylelint": "14.14.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended": "9.0.0",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "cspell",
      "prettier -c -w",
      "eslint -c .eslintrc.js --fix",
      "stylelint --cache --fix"
    ]
  },
  "packageManager": "yarn@3.2.3"
}
